{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "strict": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "emitDeclarationOnly": false
  },
  "include": ["src/**/*.ts", "src/__tests__/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
